{
  "manifest_version": 2,
  "name": "Instatrick - instragram scroll without login",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "128.png"
  }
}